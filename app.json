{
  "expo": {
    "name": "Cryptum",
    "slug": "Cryptum",
    "version": "0.1.71",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0B0B"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cryptum.png",
        "backgroundColor": "#0B0B0B"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.desaubv.cryptum"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b4c7af0e-b26f-4284-8d91-5abab94c98eb"
      }
    }
  }
}
